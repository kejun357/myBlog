<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <title>全职高手</title>
</head>
<body>
<div class="container">
    <%include header.ejs%>
    <h2 class="title">
        <span class="pattern-left"></span>
        <span class="tit">章节列表</span>
        <span class="pattern-right"></span>
    </h2>
    <ul class="detail clear">
        <% list.forEach(function (item,index) { %>
            <li>
                <a href="javascript:void(0)" target="_blank"><%- item.id +'&nbsp;' + item.title %></a>
            </li>
        <%})%>
    </ul>
    <p style="margin-top:30px;"><img src="images/demo.png" /> </p>
</div>
</body>
</html>
